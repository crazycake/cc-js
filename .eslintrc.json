{
	"extends": "eslint:recommended",
	"env": {
		"browser": true,
		"node"   : true,
		"es6"    : true
	},
	"globals": {
		"APP": false,
		"cc" : true
	},
	"parserOptions": {
		"sourceType": "module"
	},
	"rules": {
		"no-console": 0,
		"no-mixed-spaces-and-tabs": 0,
		"no-unexpected-multiline": 2,
		"linebreak-style": ["error", "unix"]
	}
}
